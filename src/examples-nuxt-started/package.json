    "@typescript-eslint/parser": "^6.2.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.6.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vue-tsc": "^1.8.6"
  },
  "dependencies": {}
