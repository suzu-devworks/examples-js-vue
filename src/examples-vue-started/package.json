{
  "name": "examples-vue-started",
  "version": "0.0.0",
  "private": "true",
  "scripts": {
    "dev": "vite --port 3000 --host 127.0.0.1",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "clean": "rimraf dist/",
    "lint:typecheck": "tsc",
    "lint:prettier": "prettier . \"!test*\" --check --ignore-path .gitignore",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.mts --ignore-path .gitignore",
    "lint": "run-p lint:*",
    "lintfix:prettier": "yarn lint:prettier --write",
    "lintfix:eslint": "yarn lint:eslint --fix",
    "lintfix": "run-s lintfix:prettier lintfix:eslint "
  },
  "dependencies": {
    "@vue/compiler-dom": "^3.2.47",
    "@vue/test-utils": "^2.3.2",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "sass": "^1.55.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.0.7"
  }
}
